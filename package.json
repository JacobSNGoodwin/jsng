{
  "name": "jsng-ghost-theme",
  "version": "0.1.0",
  "description": "blog theme for jsng",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p browsersync parcel",
    "parcel": "./node_modules/.bin/parcel watch ./theme-js/entry.js -d ./assets --out-file theme.js",
    "browsersync": "./node_modules/.bin/browser-sync start --config ./bs-config.js"
  },
  "keywords": [
    "blog",
    "theme",
    "ghost"
  ],
  "author": {
    "name": "Jacob Goodwin",
    "email": "jacob.goodwin@gmail.com"
  },
  "config": {
    "posts_per_page": 5
  },
  "license": "MIT",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.4",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "animate.css": "^3.6.1"
  }
}
